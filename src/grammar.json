{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "kee",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "statement"
      }
    },
    "statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "keybind",
          "content": {
            "type": "SYMBOL",
            "name": "keybind"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "action",
          "content": {
            "type": "SYMBOL",
            "name": "action"
          }
        }
      ]
    },
    "keybind": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9\\-]+"
    },
    "action": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "namespace"
        },
        {
          "type": "STRING",
          "value": "::"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "args"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "namespace": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-Z_]+"
    },
    "args": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "string"
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "string": {
      "type": "PATTERN",
      "value": "'[^']*'"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}